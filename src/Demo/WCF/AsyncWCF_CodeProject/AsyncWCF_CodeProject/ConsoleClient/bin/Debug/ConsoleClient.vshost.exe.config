<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <client>
      <!--<endpoint address="net.tcp://localhost:9010/MyService" binding="netTcpBinding"
        bindingConfiguration="maxtcpBinding" contract="WCFServiceContract.IMyService"
        name="MyService" />-->
      <endpoint address="net.tcp://localhost:9011/AddService" binding="netTcpBinding"
        bindingConfiguration="maxtcpBinding" contract="WCFServiceContract.IAddService"
        name="AddService" />
    </client>

    <bindings>
      <netTcpBinding>
        <binding name="maxtcpBinding" closeTimeout="00:02:00" openTimeout="00:02:00"
          receiveTimeout="00:50:00" sendTimeout="00:50:00" listenBacklog="15"
          maxConnections="15" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647"
            maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
          <security mode="Transport">
            <transport clientCredentialType="Windows" protectionLevel="None">
              <extendedProtectionPolicy policyEnforcement="Never" />
            </transport>
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>